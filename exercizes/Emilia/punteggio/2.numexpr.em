extern echo(double);

fun man(input)      // -> 22
    echo(-input):            // -22               
    echo(input*2):           // 44   
    echo(input*2 + input*3): // 110               
    echo(input*-1 + 4):      // -18       
    echo(input*2):            // 44   
    echo(0 + 0 + 0 + 0 + 0 + 0 - 0):
    echo(-3+2)            // -1   
;

